{
  "name": "engineering-thesis-lobby",
  "version": "0.0.1",
  "description": "Next.js & Koa based, system for matching students with thesis'",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "antd": "^2.13.8",
    "axios": "^0.17.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "bcrypt": "^1.0.3",
    "jsonwebtoken": "^8.1.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^2.5.0",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.3.0",
    "koa-session": "^5.5.0",
    "next": "^4.1.4",
    "next-redux-wrapper": "^1.3.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-spinners": "^0.2.5",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.19.0",
    "sequelize-cli": "^3.0.0",
    "sequelize-fixtures": "^0.7.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.2.5"
  },
  "scripts": {
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "deploy": "now --public",
    "dev": "concurrently --names \"APP,DB\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run app\" \"npm run dev-db\" --kill-others",
    "dev-db": "docker-compose -f docker-compose-dev.yml up",
    "app": "nodemon -w server.js -w routes -w controllers -w models",
    "seed": "node fixtures/loadFixtures.js",
    "test:nightwatch": "./nightwatch/run.sh",
    "test:nighwatch:seed": "node fixtures/test/loadFixtures.js"
  },
  "author": "Rafal Wilinski",
  "license": "ISC"
}
